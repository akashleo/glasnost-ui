{
  "name": "@glasnost-ui/svelte",
  "version": "0.1.6",
  "description": "Svelte components for Glasnost UI with liquid glass aesthetics",
  "keywords": [
    "svelte-glass-ui",
    "liquid-glass",
    "svelte-liquid-glass",
    "glass",
    "glassmorphism",
    "transparent",
    "glasnost",
    "svelte-glasnost"
  ],
  "homepage": "https://github.com/akashleo/glasnost-ui#readme",
  "bugs": {
    "url": "https://github.com/akashleo/glasnost-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akashleo/glasnost-ui.git"
  },
  "license": "ISC",
  "author": "Akash Ghosh",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
