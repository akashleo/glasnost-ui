  "dependencies": {},
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@types/node": "^22.10.7",
    "glob": "^10.3.10",
    "svelte": "^5.36.2",
    "vite": "^6.0.7",
    "vitest": "^2.1.8",
    "typescript": "^5.7.2"
