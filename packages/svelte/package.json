{
  "name": "@glasnost-ui/svelte",
  "version": "0.1.1",
  "description": "Svelte components for Glasnost UI with liquid glass aesthetics",
  "keywords": [
    "svelte-glass-ui",
    "liquid-glass",
    "svelte-liquid-glass",
    "glass",
    "glassmorphism",
    "transparent",
    "glasnost",
    "svelte-glasnost"
  ],
  "homepage": "https://github.com/akashleo/glasnost-ui#readme",
  "bugs": {
    "url": "https://github.com/akashleo/glasnost-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akashleo/glasnost-ui.git"
  },
  "license": "ISC",
  "author": "Akash Ghosh",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "dependencies": {
    "@glasnost-ui/shared": "workspace:0.1.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.0",
    "@types/node": "^18.0.0",
    "svelte": "^4.2.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "module": "dist/index.esm.js",
  "sideEffects": false
